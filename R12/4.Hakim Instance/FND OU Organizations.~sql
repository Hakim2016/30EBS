SELECT * FROM ORG_ORGANIZATION_DEFINITIONS OOD ORDER BY ood.ORGANIZATION_ID DESC;

SELECT * from xx where 1=1 ;

SELECT *
  FROM HR_OPERATING_UNITS HOU
 WHERE 1 = 1
 ORDER BY HOU.BUSINESS_GROUP_ID;

