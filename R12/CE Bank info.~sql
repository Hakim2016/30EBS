--bank
SELECT * from hz_parties hp where 1=1 
AND hp.party_type = 'ORGANIZATION'
--AND hp.party_name LIKE '%Bank'
ORDER BY hp.creation_date DESC --hp.PARTY_ID 
;

--bank brach
SELECT * from ce_bank_accounts t where 1=1 ORDER BY t.BANK_ACCOUNT_ID DESC;


--bank account
SELECT * from IBY_EXT_BANK_ACCOUNTS_V v where 1=1 
ORDER BY v.ext_bank_account_id DESC;
