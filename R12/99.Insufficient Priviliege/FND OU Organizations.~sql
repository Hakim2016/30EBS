SELECT * FROM apps.ORG_ORGANIZATION_DEFINITIONS OOD
WHERE 1=1
AND ood.ORGANIZATION_NAME LIKE '%_ZJ_%浙江省本部%';

SELECT *
  FROM apps.HR_OPERATING_UNITS HOU
 WHERE 1 = 1
 AND hou.name LIKE '%浙江省本部%'
 ORDER BY HOU.BUSINESS_GROUP_ID;

--Vision China
/*
organization_id 4555
*/

SELECT *
  FROM RA_CUSTOMER_TRX_ALL RCT
 WHERE 1 = 1
   AND RCT.ORG_ID = 4555;
