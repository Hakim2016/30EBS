SELECT * FROM ZX_REGIMES_B ZRB WHERE 1 = 1;

SELECT * FROM ZX.ZX_TAXES_B# ZT WHERE 1 = 1;
SELECT *
  FROM ZX_TAXES_B ZT
 WHERE 1 = 1
   AND ZT.EFFECTIVE_TO IS NULL;

SELECT * FROM ZX_STATUS_B ZS WHERE 1 = 1;

SELECT *
  FROM --ZX_JURISDICTIONS_VL
       ZX_JURISDICTIONS_B ZJ
 WHERE 1 = 1;

--DETERMINING_FACTOR
SELECT *
  FROM --zx.ZX_RECOVERY_TYPES_B# 
       --ZX_RECOVERY_TYPES_VL
        ZX.ZX_RECOVERY_TYPES_TL ZR
 WHERE 1 = 1;

SELECT ZDFV.DETERMINING_FACTOR_CLASS_CODE,
       ZDFV.DETERMINING_FACTOR_NAME,
       ZDFV.DETERMINING_FACTOR_DESC,
       ZDFB.CREATED_BY,
       (SELECT F.USER_NAME
          FROM FND_USER F
         WHERE 1 = 1
           AND F.USER_ID = ZDFB.CREATED_BY) CREATOR,
       ZDFB.*
  FROM ZX_DETERMINING_FACTORS_B ZDFB, ZX_DETERMINING_FACTORS_VL ZDFV

 WHERE 1 = 1
   AND ZDFB.DETERMINING_FACTOR_CODE = ZDFV.DETERMINING_FACTOR_CODE --ZDFV.DETERMINING_FACTOR_CLASS_CODE

;

--Determining factor sets
SELECT * FROM ZX.ZX_DETERMINING_FACTORS_B#;

SELECT (SELECT F.USER_NAME
          FROM FND_USER F
         WHERE 1 = 1
           AND F.USER_ID = ZC.CREATED_BY) CREATOR,
       ZC.ALPHANUMERIC_VALUE,
       ZC.VALUE_LOW,
       ZC.*
  FROM ZX_CONDITIONS ZC
 WHERE 1 = 1
--AND zc.
;

--纳税条件集
SELECT * FROM ZX_CONDITION_GROUPS_B
;
